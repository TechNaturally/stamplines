{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "semi": "error",
        "strict": "error",
        "indent": ["warn", 2],
        "brace-style": ["warn", "stroustrup"],
        "keyword-spacing": "warn",
        "space-before-blocks": "warn",
        "quotes": ["warn", "single"],
        "prefer-arrow-callback": "warn",
        "eol-last": "error"
    }
}
